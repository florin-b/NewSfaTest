<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    
    <item android:state_pressed="true"><shape>
            <solid android:color="#939E7D" />

            <corners android:radius="5dp" />

            <stroke android:width="1dp" android:color="#50A186" />

            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape></item>
    <item><shape xmlns:android="http://schemas.android.com/apk/res/android" android:shape="rectangle">
            <gradient android:angle="270" android:endColor="#DAEBB9" android:startColor="#EEF3E2" />

            <corners android:radius="5dp" />

            <stroke android:width="1dp" android:color="#50A186" />

            <padding android:bottom="10dp" android:left="10dp" android:right="10dp" android:top="10dp" />
        </shape></item>

</selector>