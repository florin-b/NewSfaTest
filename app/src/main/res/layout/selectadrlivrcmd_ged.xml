<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android" >

    <ScrollView
        android:layout_width="fill_parent"
        android:layout_height="fill_parent" >

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:focusableInTouchMode="true"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:text="Transport"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <Spinner
                    android:id="@+id/spinnerTransp"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="left|bottom"
                    android:layout_weight="0.1"
                    android:gravity="left" />

                <CheckBox
                    android:id="@+id/checkMacara"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_gravity="left|bottom"
                    android:layout_marginLeft="30dp"
                    android:layout_weight="0.15"
                    android:text="Camion cu macara" />

                <Spinner
                    android:id="@+id/spinnerTonaj"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="left|bottom"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.1"
                    android:gravity="left" />
            </LinearLayout>


            <LinearLayout
                android:id="@+id/layoutFilLivrare"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|left"
                    android:gravity="left"
                    android:paddingLeft="5dip"
                    android:text="Filiala livrare"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <Spinner
                    android:id="@+id/spinnerFiliale"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="left|bottom"
                    android:layout_weight="0.3"
                    android:gravity="left" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.7" />

            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutHeaderAdrese"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dip"
                android:orientation="horizontal" >

                <View
                    android:layout_width="150dp"
                    android:layout_height="45dip" />

                <RadioGroup
                    android:id="@+id/radioAdresa"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal" >

                    <RadioButton
                        android:id="@+id/radioListAdrese"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:checked="true"
                        android:text="Selecteaza adresa din lista"
                        android:textColor="@color/labelColor"
                        android:textSize="16sp" />

                    <RadioButton
                        android:id="@+id/radioTextAdrese"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="30dip"
                        android:text="Introdu adresa noua"
                        android:textColor="@color/labelColor"
                        android:textSize="16sp" />
                </RadioGroup>

                <View
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.25" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutListAdrese"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="150dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:gravity="left"
                    android:paddingLeft="5dip"
                    android:text="Adresa livrare"
                    android:textColor="@color/labelColor"
                    android:textSize="16sp" />

                <Spinner
                    android:id="@+id/spinnerAdreseLivrare"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="left|bottom"
                    android:layout_weight="0.25"
                    android:background="@drawable/dropdown_style_1"
                    android:gravity="left" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="45dip"
                    android:layout_weight="0.2" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutAdr1"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:focusable="true"
                android:focusableInTouchMode="true"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:text="Adresa livrare"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <Spinner
                    android:id="@+id/spinnerJudet"
                    android:layout_width="0dp"
                    android:layout_height="45dip"
                    android:layout_gravity="bottom|left"
                    android:layout_weight="0.1"
                    android:clickable="true"
                    android:gravity="left" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginRight="20dp"
                    android:layout_weight="0.05"
                    android:gravity="right"
                    android:text="Localitate"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <AutoCompleteTextView
                    android:id="@+id/autoCompleteLocalitate"
                    android:layout_width="0dp"
                    android:layout_height="45dip"
                    android:layout_gravity="bottom|left"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.1"
                    android:dropDownHeight="250dp"
                    android:focusable="true"
                    android:focusableInTouchMode="true"
                    android:inputType="textCapSentences|textNoSuggestions"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutAdr2"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:text="Strada"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <AutoCompleteTextView
                    android:id="@+id/autoCompleteStrada"
                    android:layout_width="0dp"
                    android:layout_height="45dip"
                    android:layout_gravity="bottom|left"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.15"
                    android:dropDownHeight="250dp"
                    android:inputType="textCapSentences|textNoSuggestions"
                    android:maxLength="80"
                    android:singleLine="true"
                    android:textSize="16sp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginRight="20dp"
                    android:layout_weight="0.05"
                    android:gravity="right"
                    android:text="Nr"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <EditText
                    android:id="@+id/textNrStr"
                    android:layout_width="0dp"
                    android:layout_height="45dip"
                    android:layout_gravity="bottom|left"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.05"
                    android:dropDownHeight="250dp"
                    android:inputType="textCapSentences|textNoSuggestions"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutAdr3"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:text="Pozitionare"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <Button
                    android:id="@+id/btnPozitieAdresa"
                    style="@style/ok_button"
                    android:layout_width="0dip"
                    android:layout_height="40dip"
                    android:layout_gravity="bottom|left"
                    android:layout_weight="0.2"
                    android:background="@drawable/cancel_button_style"
                    android:paddingLeft="20dip"
                    android:paddingRight="20dip"
                    android:text="Pozitionati adresa pe harta" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.25" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:text="Pers. contact"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <EditText
                    android:id="@+id/txtPersCont"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.25"
                    android:gravity="left"
                    android:inputType="textNoSuggestions"
                    android:maxLength="65"
                    android:singleLine="true"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left|center"
                    android:text="Telefon"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <EditText
                    android:id="@+id/txtTelefon"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="bottom|left"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.25"
                    android:gravity="bottom"
                    android:inputType="number"
                    android:maxLength="65"
                    android:singleLine="true"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutMail"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left|center"
                    android:text="E-mail"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <EditText
                    android:id="@+id/txtMail"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="bottom|left"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.25"
                    android:gravity="bottom"
                    android:inputType="text"
                    android:maxLength="150"
                    android:singleLine="true"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutMeseriasi"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal"
                android:visibility="gone" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left|center"
                    android:text="Meserias"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <Spinner
                    android:id="@+id/spinnerMeseriasi"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="left|bottom"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.25"
                    android:gravity="left" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left|center"
                    android:text="Doc.insotitor"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <CheckBox
                    android:id="@+id/checkFactura"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_gravity="left|bottom"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="20dip"
                    android:layout_weight="0.15"
                    android:text="Factura" />

                <CheckBox
                    android:id="@+id/checkAviz"
                    android:layout_width="0dip"
                    android:layout_height="match_parent"
                    android:layout_gravity="left|bottom"
                    android:layout_marginLeft="30dp"
                    android:layout_marginRight="20dip"
                    android:layout_weight="0.35"
                    android:text="Aviz de expeditie" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:text="Plata"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <Spinner
                    android:id="@+id/spinnerPlata"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="left|bottom"
                    android:layout_weight="0.17"
                    android:gravity="left" />

                <TextView
                    android:id="@+id/tipPlataContract"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.1"
                    android:layout_gravity="center_vertical"
                    android:gravity="center"
                    android:textSize="16sp" />

                <Spinner
                    android:id="@+id/spinnerTermenPlata"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="left|bottom"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.1" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutFilialaPlata"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal"
                android:visibility="gone">

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|left"
                    android:gravity="left"
                    android:paddingLeft="5dip"
                    android:text="Plata in filiala"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <RadioGroup
                    android:id="@+id/radioFilialaPlata"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|left"
                    android:orientation="horizontal">

                    <RadioButton
                        android:id="@+id/radioPlataFilialaAg"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:textColor="@color/labelColor"
                        android:textSize="17sp" />

                    <RadioButton
                        android:id="@+id/radioPlataFilialaLivrare"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="100dp"
                        android:textColor="@color/labelColor"
                        android:textSize="17sp" />
                </RadioGroup>


            </LinearLayout>


            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:text="Data livrare"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <TextView
                    android:id="@+id/textDataLivrare"
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <Button
                    android:id="@+id/btnDataLivrare"
                    style="@style/ok_button"
                    android:layout_width="0dip"
                    android:layout_height="40dip"
                    android:layout_gravity="bottom|left"
                    android:layout_weight="0.2"
                    android:background="@drawable/cancel_button_style"
                    android:paddingLeft="20dip"
                    android:paddingRight="20dip"
                    android:text="Selectati data livrare" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.35" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:gravity="left|bottom"
                    android:paddingLeft="5dip"
                    android:text="Facturare paleti"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <CheckBox
                    android:id="@+id/chkFactPaleti"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:checked="false"
                    android:text="Clientul nu doreste paletii" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:gravity="left|bottom"
                    android:paddingLeft="5dip"
                    android:text="Camion descoperit"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <CheckBox
                    android:id="@+id/chkCamionDescoperit"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:checked="false"
                    android:text="Camion descoperit" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:text="Obs. sofer"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <CheckBox
                    android:id="@+id/chkObsSofer"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginRight="15dip"
                    android:checked="false"
                    android:text="Nu" />

                <EditText
                    android:id="@+id/txtObservatii"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.25"
                    android:gravity="left"
                    android:inputType="textNoSuggestions"
                    android:maxLength="100"
                    android:singleLine="true"
                    android:textSize="18sp"
                    android:visibility="invisible" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutClientRaft"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:gravity="left|bottom"
                    android:paddingLeft="5dip"
                    android:text="Client la raft"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <CheckBox
                    android:id="@+id/clientRaft"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:text="Nu" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutIndoire"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:gravity="left"
                    android:paddingLeft="5dip"
                    android:text="Prelucrare"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <Spinner
                    android:id="@+id/spinnerIndoire"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="left|bottom"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.13"
                    android:gravity="left" />

                <Spinner
                    android:id="@+id/spinnerDebitare"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="left|bottom"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.13"
                    android:gravity="left" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.1" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:text="Obs. plata"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <EditText
                    android:id="@+id/txtObsPlata"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.25"
                    android:gravity="left"
                    android:inputType="textNoSuggestions"
                    android:maxLength="100"
                    android:singleLine="true"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutProgramLivrare"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:layout_marginBottom="20dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:gravity="left"
                    android:paddingLeft="5dip"
                    android:text="Perioada livrare"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <Spinner
                    android:id="@+id/spinnerProgramLivrare"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_gravity="left|bottom"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.2"
                    android:entries="@array/programLivrareItems"
                    android:gravity="left" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.3" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutOptiuniAdrLivrare"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal"
                android:visibility="gone"
                >

                <TextView
                    android:id="@+id/labelAdresa22"
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <RadioGroup
                    android:id="@+id/radioAdresaLivrare"
                    android:layout_width="wrap_content"
                    android:layout_height="45dip"
                    android:layout_gravity="bottom"
                    android:orientation="horizontal"
                    >

                    <RadioButton
                        android:id="@+id/radioAdresaSediu"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="Adresa de livrare"

                        android:textColor="@color/labelColor"
                        android:textSize="16sp" />

                    <RadioButton
                        android:id="@+id/radioAltaAdresa"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dip"
                        android:text="Alta adresa"
                        android:checked="true"
                        android:textColor="@color/labelColor"
                        android:textSize="16sp" />

                    <RadioButton
                        android:id="@+id/radioAdresaObiectiv"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="20dip"
                        android:text="Adresa obiectiv"
                        android:textColor="@color/labelColor"
                        android:textSize="16sp"
                        android:visibility="gone" />
                </RadioGroup>

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.2" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutAdrLivrare1"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:id="@+id/labelAdresa2"
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="left|center"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <Spinner
                    android:id="@+id/spinnerJudetLivrare"
                    android:layout_width="0dp"
                    android:layout_height="45dip"
                    android:layout_gravity="bottom|left"
                    android:layout_weight="0.1"
                    android:gravity="left" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical|left"
                    android:layout_marginLeft="30dp"
                    android:layout_weight="0.05"
                    android:gravity="left"
                    android:text="Localitate"
                    android:textColor="@color/labelColor"
                    android:textSize="16sp" />

                <AutoCompleteTextView
                    android:id="@+id/autoCompleteLocLivrare"
                    android:layout_width="0dp"
                    android:layout_height="45dip"
                    android:layout_gravity="bottom|left"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.15"
                    android:dropDownHeight="250dp"
                    android:inputType="textCapSentences|textNoSuggestions"
                    android:textSize="18sp" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutAdrLivrare2"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:text="Strada"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <AutoCompleteTextView
                    android:id="@+id/autoCompleteStradaLivrare"
                    android:layout_width="0dp"
                    android:layout_height="45dip"
                    android:layout_gravity="bottom|left"
                    android:layout_marginRight="180dip"
                    android:layout_weight="0.2"
                    android:dropDownHeight="250dp"
                    android:inputType="textCapSentences|textNoSuggestions"
                    android:maxLength="80"
                    android:singleLine="true"
                    android:textSize="16sp" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:orientation="horizontal" >

                <View
                    android:layout_width="0dp"
                    android:layout_height="80dip"
                    android:layout_weight="0.3" />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/layoutObiectiveConsilieri"
                android:layout_width="match_parent"
                android:layout_height="60dip"
                android:layout_marginBottom="100dip"
                android:orientation="horizontal"
                android:visibility="gone" >

                <TextView
                    android:layout_width="170dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginLeft="3dip"
                    android:gravity="left"
                    android:text="Obiectiv"
                    android:textColor="@color/labelColor"
                    android:textSize="17sp"
                    android:typeface="serif" />

                <TextView
                    android:id="@+id/textObiectivSelectat"
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_gravity="bottom|left"
                    android:layout_marginLeft="3dip"
                    android:layout_weight="0.6"
                    android:gravity="left"
                    android:textColor="@color/black"
                    android:textSize="17sp"
                    android:typeface="serif"
                    android:visibility="gone" />

                <ImageButton
                    android:id="@+id/btnStergeObiectiv"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:layout_marginLeft="20dip"
                    android:layout_marginRight="20dip"
                    android:background="?android:attr/selectableItemBackground"
                    android:src="@drawable/cancel_icon_32"
                    android:visibility="gone" />

                <Button
                    android:id="@+id/btnSelectObiectiv"
                    style="@style/ok_button"
                    android:layout_width="250dip"
                    android:layout_height="40dip"
                    android:layout_gravity="bottom|left"
                    android:background="@drawable/cancel_button_style"
                    android:paddingLeft="20dip"
                    android:paddingRight="20dip"
                    android:text="Selectati un obiectiv" />

                <View
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:layout_weight="0.15" />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

